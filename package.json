{
  "name": "react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.4.5",
    "core-js": "3.0.1",
    "ramda": "^0.26.1",
    "ramda-adjunct": "^2.18.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.1",
    "react-scripts": "3.0.1",
    "regenerator-runtime": "^0.13.2",
    "styled-components": "^4.3.0",
    "styled-map": "^3.3.0",
    "styled-normalize": "^8.0.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "cov": "react-scripts test --coverage",
    "format": "standard 'src/**/*.js' --fix"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "globals": [
      "fetch",
      "toJson"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "airkro"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "!<rootDir>/node_modules/",
      "!src/index.js",
      "!src/serviceWorker.js",
      "src/**/*.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "husky": "^2.4.0",
    "import-sort": "^6.0.0",
    "import-sort-style-airkro": "^0.1.7",
    "jest-dom": "^3.4.0",
    "jest-enzyme": "^7.0.2",
    "jest-in-case": "^1.0.2",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.2.0",
    "npm-quick-run": "^1.16.1",
    "standard": "^12.0.1"
  }
}
